{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "backend",
      "host": {
        "sourcePath": "/var/app/current/backend"
      }
    },
    {
      "name": "nginx",
      "host": {
        "sourcePath": "/var/app/current/proxy/conf.d"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "076729327075.dkr.ecr.us-west-2.amazonaws.com/clairvoyance_backend",
      "essential": true,
      "mountPoints": [
        {
          "containerPath": "/app/backend",
          "sourceVolume": "backend"
        }
      ],
      "memory": 128
    },
    {
      "essential": true,
      "image": "076729327075.dkr.ecr.us-west-2.amazonaws.com/clairvoyance_nginx",
      "mountPoints": [
        {
          "containerPath": "/etc/nginx/conf.d",
          "sourceVolume": "nginx"
        }
      ],
      "name": "nginx",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 8000
        }
      ],
      "memory": 128
    }
  ]
}
